<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="Party" package="seerp">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="name" type="text-medium"/>
        <field name="address" type="text-long"/>
        <field name="mobileNumber" type="text-medium"/>
        <field name="email" type="text-medium"/>
        <field name="aadharNumber" type="text-medium"/>
        <field name="gstNumber" type="text-medium"/>
        <field name="accountNumber" type="text-medium"/>
    </entity>
</entities>